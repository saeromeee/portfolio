/* CSS */
.pageTitle {position: fixed; margin-bottom:600px; left: 0; top: 0; width: 100%; height: 50px; line-height: 50px; color: black; text-align: center; background: white;}
<!-- div .infinity {padding: 52px 3% 0;} -->
div .container div{margin-bottom:10px;}
div .container div .box {width:40px;height:40px;border-radius:70%;overflow:hidden;background:#BDBDBD;}
div .container div .box .profile {width:100%;height:100%;object-fit:cover;display:flex;}

div .container:nth-child(2n+1) {background: #F4F6F6; margin-top:50px; margin-bottom:50px; padding: 20px; min-height: 500px;}
div .container:nth-child(2n+1) p {line-height: 22px; color: black; font-size: 16px; font-weight: 600;}
div .container:nth-child(2n) {background: #F4F6F6; margin-top:50px; margin-bottom:50px; padding: 20px; min-height: 500px; }
div .container:nth-child(2n) p {line-height: 22px; color: black; font-size: 16px; font-weight: 600;}

